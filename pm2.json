{
    "apps": [{
        "name": "ha-ds",
        "script": "./bin/www",
        "watch": false,
        "log_date_format": "YYYY-MM-DD HH:mm Z",
        "out_file": "/var/log/ha-ds-out.log",
        "error_file": "/var/log/ha-ds-err.log",
        "env": {
            "DEBUG": "app:*",
            "NODE_ENV": "development"
        },
        "env_production": {
            "DEBUG": "app:error",
            "NODE_ENV": "production"
        }
    }]
}
